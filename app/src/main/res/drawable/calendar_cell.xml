<?xml version="1.0" encoding="utf-8"?>
<level-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Default Cell -->
    <item android:maxLevel="0">
        <shape android:shape="oval">
            <solid android:color="@color/cellDefault"/>
            <padding android:bottom="2dp" android:left="2dp" android:right="2dp" android:top="2dp"/>
            <stroke android:color="@android:color/transparent"/>
        </shape>
    </item>

    <!-- Default Cell Event -->
    <item android:maxLevel="1">
        <shape android:shape="oval">
            <solid android:color="@color/cellEvent"/>
            <padding android:bottom="2dp" android:left="2dp" android:right="2dp" android:top="2dp"/>
            <stroke android:color="@android:color/transparent"/>
        </shape>
    </item>

    <!-- Default Cell Selected -->
    <item android:maxLevel="2">
        <shape android:shape="oval">
            <solid android:color="@color/cellDefault"/>
            <padding android:bottom="2dp" android:left="2dp" android:right="2dp" android:top="2dp"/>
            <stroke android:color="@color/navigationBar" android:width="3dp"/>
        </shape>
    </item>

    <!-- Default Cell Event Selected -->
    <item android:maxLevel="3">
        <shape android:shape="oval">
            <solid android:color="@color/cellEvent"/>
            <padding android:bottom="2dp" android:left="2dp" android:right="2dp" android:top="2dp"/>
            <stroke android:color="@color/navigationBar" android:width="3dp"/>
        </shape>
    </item>

    <!-- Today Cell -->
    <item android:maxLevel="4">
        <shape android:shape="oval">
            <solid android:color="@color/cellDefault"/>
            <padding android:bottom="2dp" android:left="2dp" android:right="2dp" android:top="2dp"/>
            <stroke android:color="@color/cellTodayBorder" android:width="2dp"/>
        </shape>
    </item>

    <!-- Today Cell Event -->
    <item android:maxLevel="5">
        <shape android:shape="oval">
            <solid android:color="@color/cellEvent"/>
            <padding android:bottom="2dp" android:left="2dp" android:right="2dp" android:top="2dp"/>
            <stroke android:color="@color/cellTodayBorder" android:width="2dp"/>
        </shape>
    </item>

    <!-- Today Cell Selected -->
    <item android:maxLevel="6">
        <shape android:shape="oval">
            <solid android:color="@color/cellDefault"/>
            <padding android:bottom="2dp" android:left="2dp" android:right="2dp" android:top="2dp"/>
            <stroke android:color="@color/cellTodayBorder" android:width="3dp"/>
        </shape>
    </item>

    <!-- Today Cell Event Selected -->
    <item android:maxLevel="7">
        <shape android:shape="oval">
            <solid android:color="@color/cellEvent"/>
            <padding android:bottom="2dp" android:left="2dp" android:right="2dp" android:top="2dp"/>
            <stroke android:color="@color/cellTodayBorder" android:width="3dp"/>
        </shape>
    </item>

</level-list>

